<div class="row">
{{#each places as |place|}}
    <div class="col s4 m4">
      <div class="card">
        <div class="card-image medium">
          <a href="/places/{{place._id}}">
            <img src="{{place.image}}" alt="{{place.name}}">
          </a>
          <a href="/places/{{place._id}}">
            <span class="card-title">{{place.name}}</span>
          </a>
          <a href="/places/{{_id}}/edit" class="btn-floating halfway-fab waves-effect waves-light red"><i class="material-icons">edit</i></a>
        </div>
        <div class="card-action">
          <a class="white-text btn blue" href="/places/{{_id}}/edit">Edit</a>
          <a class="white-text btn red" href="/places/{{_id}}/delete">Delete</a>
        </div>
      </div>
    </div>
{{/each}}
<div class="fixed-action-btn">
  <a href="/places/new" class="btn-floating btn-large red">
    <i class="material-icons">add_circle_outline</i>
  </a>
  <ul>
    <li><a class="btn-floating red"><i class="material-icons">insert_chart</i></a></li>
</ul>
</div>
</div>